//一个workflow有N个task组成,这里是workflow执行器配置
{
    //这里相当于为每个屏幕定义需要的task flow
    "login":{
        "task-flow":[
            //key=wait-next-screen 关键字表示等待下一个界面出现
            {"task-name":"login","flow":[{"key":"手机号","value":"$phone_num"},{"key":"密码","value":"$pwd"},{"key":"登陆","value":""},{"key":"wait-next-screen","value":"发现"}]},
            {"task-nmae":"goto-register","flow":[{"key":"注册","value":""},{"key":"wati-next-screen","value":"register"}]}
        ]
    },
    "register":{
    },
    "me":{
        "task-flow":[
            //先向上拉屏幕；休息一秒钟等待屏幕同步；触发对应menu
            {"task-name":"领取奖励","flow":[{"key":"drag-up","value":""},{"key":"sleep","value":1},{"key":"领取奖励","value":""}]},
            {"task-name":"goto-setup","flow":[{"key":"drag-up","value":""},{"key":"sleep","value":1},{"key":"设置","value":""},{"key":"wati-next-screen","value":"设置"}]}
        ]
    },
    "setup":{
        "task-flow":[
            {"task-name":"goto-login","flow":[{"key":"切换帐号","value":""},{"key":"wait-pop-meun","value":"bottom-pop-menu"},{"key":"bottom-pop-menu.退出登录","value":""},{"key":"wait-next-screen","value":"login"}]}
        ]
    }
}
